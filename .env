PORT=3000
DB_HOST='YOUR_POSTGRES_HOST'
DB_USERNAME='YOUR_POSTGRES_USERNAME'
DB_PASSWORD='YOUR_POSTGRES_PASSWORD'
DB_DATABASE='YOUR_POSTGRES_DATABASE'
DB_DAILECT='postgres'
